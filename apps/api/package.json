{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack --webpackPath webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate:migration": "drizzle-kit generate:pg --config=drizzle-config.ts",
    "run:migration": "ts-node run-migrations.ts",
    "run:drizzle-push": "drizzle-kit push:pg --config=drizzle-config.ts",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "e2e": "jest --config ./test/api-e2e.json",
    "e2e:watch": "jest --config ./test/api-e2e.json --watchAll"
  },
  "dependencies": {
    "@myapp/entities": "workspace:*",
    "@myapp/types-contract": "workspace:*",
    "@myapp/ui": "workspace:*",
    "@nestjs/apollo": "10.0.19",
    "@nestjs/common": "9.0.8",
    "@nestjs/core": "9.0.8",
    "@nestjs/graphql": "10.0.21",
    "@nestjs/platform-express": "9.0.8",
    "@ts-rest/nest": "^3.30.3",
    "apollo-server-express": "3.6.7",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "graphql": "16.5.0",
    "nestjs": "^0.0.1",
    "postgres": "^3.3.5",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5"
  },
  "devDependencies": {
    "@myapp/nest-config": "workspace:*",
    "@myapp/tsconfig": "workspace:*",
    "@nestjs/cli": "8.2.8",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "9.0.8",
    "@types/express": "4.17.13",
    "@types/node": "16.0.0",
    "drizzle-kit": "^0.19.13",
    "jest": "28.1.2",
    "prettier": "2.7.1",
    "run-script-webpack-plugin": "0.1.1",
    "source-map-support": "0.5.21",
    "supertest": "6.2.3",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.1",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "bundledDependencies": [
    "@myapp/nest-config"
  ]
